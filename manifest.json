{
	"name": "Skip to Recipe",
	"description": "Skip all those annoying blogs and stories and go straight to the recipes every time.",
	"version": "0.1.1",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["tabs", "scripting"],
	"host_permissions": ["<all_urls>"],
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"64": "icons/64.png",
		"128": "icons/128.png"
	}
}
